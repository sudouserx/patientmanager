[flake8]
max-line-length = 88
ignore =
    E203,
    E266,
    E501,
    W503,
    F401,
    F403,
    F405
exclude =
    .git,
    __pycache__,
    migrations,
    venv,
    env,
    .venv,
    node_modules,
    staticfiles,
    media,
    .env,
    .env.local,
    .env.dev,
    .env.prod,
    settings/local.py,
    settings/dev.py
max-complexity = 10